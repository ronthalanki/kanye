{
	"manifest_version": 2,
	"name": "Kanye a Day",
	"description": "Displays a Kanye Quote Every Day",
	"version": "1",
	"author": "Sairanjith (Ron) Thalanki",
	"browser_action": {
   		"default_icon": "kanye.png",
   		"default_title": "What Would Kanye Do?"
 	},
	"chrome_url_overrides" : {
  		"newtab": "newtab.html"
	},
 	"permissions": ["activeTab"]
}